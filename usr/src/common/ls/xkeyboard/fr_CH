keycode 0xa =  1 plus VoidSymbol bar
keycode 0xb =  2 quotedbl VoidSymbol at
keycode 0xc =  3 asterisk VoidSymbol numbersign
keycode 0xd =  4 ccedilla  
keycode 0xe =  5 percent  
keycode 0xf =  6 ampersand VoidSymbol notsign
keycode 0x10 =  7 slash VoidSymbol brokenbar
keycode 0x11 =  8 parenleft VoidSymbol cent
keycode 0x12 =  9 parenright  
keycode 0x13 =  0 equal  
keycode 0x14 =  apostrophe question VoidSymbol usldead_acute
keycode 0x15 =  usldead_asciicircum usldead_grave VoidSymbol usldead_asciitilde
keycode 0x18 =  q Q  
keycode 0x19 =  w W  
keycode 0x1a =  e E  
keycode 0x1b =  r R  
keycode 0x1c =  t T  
keycode 0x1d =  z Z  
keycode 0x1e =  u U  
keycode 0x1f =  i I  
keycode 0x20 =  o O  
keycode 0x21 =  p P  
keycode 0x22 =  egrave udiaeresis VoidSymbol bracketleft
keycode 0x23 =  usldead_diaeresis exclam VoidSymbol bracketright
keycode 0x26 =  a A  
keycode 0x27 =  s S  
keycode 0x28 =  d D  
keycode 0x29 =  f F  
keycode 0x2a =  g G  
keycode 0x2b =  h H  
keycode 0x2c =  j J  
keycode 0x2d =  k K  
keycode 0x2e =  l L  
keycode 0x2f =  eacute odiaeresis  
keycode 0x30 =  agrave adiaeresis VoidSymbol braceleft
keycode 0x31 =  section degree  
keycode 0x33 =  dollar sterling VoidSymbol braceright
keycode 0x34 =  y Y  
keycode 0x35 =  x X  
keycode 0x36 =  c C  
keycode 0x37 =  v V  
keycode 0x38 =  b B  
keycode 0x39 =  n N  
keycode 0x3a =  m M  
keycode 0x3b =  comma semicolon  
keycode 0x3c =  period colon  
keycode 0x3d =  minus underscore  
keycode 0x41 =  space space  
keycode 0x42 =  Shift_Lock
keycode 0x5e =  less greater VoidSymbol backslash
keycode 0x78 =  Mode_switch

clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3

add shift = Shift_L
add shift = Shift_R
add shift = Alt_R
add lock = Shift_Lock
add control = Control_L

add mod1 = Alt_L
add mod1 = Alt_R
add mod1 = Mode_switch
add mod2 = Num_Lock
add mod2 = Mode_switch
add mod3 = Alt_R
add mod3 = Mode_switch

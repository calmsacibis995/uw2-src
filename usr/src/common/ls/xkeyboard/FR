keycode 0xa =  ampersand 1  
keycode 0xb =  eacute 2 VoidSymbol asciitilde
keycode 0xc =  quotedbl 3 VoidSymbol numbersign
keycode 0xd =  apostrophe 4 VoidSymbol braceleft
keycode 0xe =  parenleft 5 VoidSymbol bracketleft
keycode 0xf =  minus 6 VoidSymbol bar
keycode 0x10 =  egrave 7 VoidSymbol grave
keycode 0x11 =  underscore 8 VoidSymbol backslash
keycode 0x12 =  ccedilla 9 VoidSymbol asciicircum
keycode 0x13 =  agrave 0 VoidSymbol at
keycode 0x14 =  parenright degree VoidSymbol bracketright
keycode 0x15 =  equal plus VoidSymbol braceright
keycode 0x18 =  a A  
keycode 0x19 =  z Z  
keycode 0x1a =  e E  
keycode 0x1b =  r R  
keycode 0x1c =  t T  
keycode 0x1d =  y Y  
keycode 0x1e =  u U  
keycode 0x1f =  i I  
keycode 0x20 =  o O  
keycode 0x21 =  p P  
keycode 0x22 =  usldead_asciicircum usldead_diaeresis  
keycode 0x23 =  dollar sterling VoidSymbol currency
keycode 0x26 =  q Q  
keycode 0x27 =  s S  
keycode 0x28 =  d D  
keycode 0x29 =  f F  
keycode 0x2a =  g G  
keycode 0x2b =  h H  
keycode 0x2c =  j J  
keycode 0x2d =  k K  
keycode 0x2e =  l L  
keycode 0x2f =  m M  
keycode 0x30 =  ugrave percent  
keycode 0x31 =  twosuperior   
keycode 0x33 =  asterisk mu  
keycode 0x34 =  w W  
keycode 0x35 =  x X  
keycode 0x36 =  c C  
keycode 0x37 =  v V  
keycode 0x38 =  b B  
keycode 0x39 =  n N  
keycode 0x3a =  comma question  
keycode 0x3b =  semicolon period  
keycode 0x3c =  colon slash  
keycode 0x3d =  exclam section  
keycode 0x41 =  space space  
keycode 0x42 =  Shift_Lock
keycode 0x5e =  less greater  
keycode 0x78 =  Mode_switch

clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3

add shift = Shift_L
add shift = Shift_R
add shift = Alt_R
add lock = Shift_Lock
add control = Control_L

add mod1 = Alt_L
add mod1 = Alt_R
add mod1 = Mode_switch
add mod2 = Num_Lock
add mod2 = Mode_switch
add mod3 = Alt_R
add mod3 = Mode_switch

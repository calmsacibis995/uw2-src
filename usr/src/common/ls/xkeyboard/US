keycode 0xa =  1 exclam  
keycode 0xb =  2 at  
keycode 0xc =  3 numbersign  
keycode 0xd =  4 dollar  
keycode 0xe =  5 percent  
keycode 0xf =  6 asciicircum  
keycode 0x10 =  7 ampersand  
keycode 0x11 =  8 asterisk  
keycode 0x12 =  9 parenleft  
keycode 0x13 =  0 parenright  
keycode 0x14 =  minus underscore  
keycode 0x15 =  equal plus  
keycode 0x18 =  q Q  
keycode 0x19 =  w W  
keycode 0x1a =  e E  
keycode 0x1b =  r R  
keycode 0x1c =  t T  
keycode 0x1d =  y Y  
keycode 0x1e =  u U  
keycode 0x1f =  i I  
keycode 0x20 =  o O  
keycode 0x21 =  p P  
keycode 0x22 =  bracketleft braceleft  
keycode 0x23 =  bracketright braceright  
keycode 0x26 =  a A  
keycode 0x27 =  s S  
keycode 0x28 =  d D  
keycode 0x29 =  f F  
keycode 0x2a =  g G  
keycode 0x2b =  h H  
keycode 0x2c =  j J  
keycode 0x2d =  k K  
keycode 0x2e =  l L  
keycode 0x2f =  semicolon colon  
keycode 0x30 =  apostrophe quotedbl  
keycode 0x31 =  grave asciitilde  
keycode 0x33 =  backslash bar  
keycode 0x34 =  z Z  
keycode 0x35 =  x X  
keycode 0x36 =  c C  
keycode 0x37 =  v V  
keycode 0x38 =  b B  
keycode 0x39 =  n N  
keycode 0x3a =  m M  
keycode 0x3b =  comma less  
keycode 0x3c =  period greater  
keycode 0x3d =  slash question  
keycode 0x41 =  space space  
keycode 0x78 =  Mode_switch
keycode 122 = Alt_L

clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3

add shift = Shift_L
add shift = Shift_R
add shift = Alt_R
add lock = Caps_Lock
add control = Control_L

add mod1 = Alt_L
add mod2 = Num_Lock
add mod2 = Mode_switch

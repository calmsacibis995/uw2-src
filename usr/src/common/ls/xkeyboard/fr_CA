keycode 0xa =  1 exclam VoidSymbol plusminus
keycode 0xb =  2 quotedbl VoidSymbol at
keycode 0xc =  3 slash VoidSymbol sterling
keycode 0xd =  4 dollar VoidSymbol cent
keycode 0xe =  5 percent VoidSymbol currency
keycode 0xf =  6 question VoidSymbol notsign
keycode 0x10 =  7 ampersand VoidSymbol brokenbar
keycode 0x11 =  8 asterisk VoidSymbol twosuperior
keycode 0x12 =  9 parenleft VoidSymbol threesuperior
keycode 0x13 =  0 parenright VoidSymbol onequarter
keycode 0x14 =  minus underscore VoidSymbol onehalf
keycode 0x15 =  equal plus VoidSymbol threequarters
keycode 0x18 =  q Q  
keycode 0x19 =  w W  
keycode 0x1a =  e E  
keycode 0x1b =  r R  
keycode 0x1c =  t T  
keycode 0x1d =  y Y  
keycode 0x1e =  u U  
keycode 0x1f =  i I  
keycode 0x20 =  o O VoidSymbol section
keycode 0x21 =  p P VoidSymbol paragraph
keycode 0x22 =  usldead_asciicircum usldead_asciicircum VoidSymbol bracketleft
keycode 0x23 =  usldead_cedilla usldead_diaeresis VoidSymbol bracketright
keycode 0x26 =  a A  
keycode 0x27 =  s S  
keycode 0x28 =  d D  
keycode 0x29 =  f F  
keycode 0x2a =  g G  
keycode 0x2b =  h H  
keycode 0x2c =  j J  
keycode 0x2d =  k K  
keycode 0x2e =  l L  
keycode 0x2f =  semicolon colon VoidSymbol asciitilde
keycode 0x30 =  usldead_grave usldead_grave VoidSymbol braceleft
keycode 0x31 =  numbersign bar VoidSymbol backslash
keycode 0x33 =  less greater VoidSymbol braceright
keycode 0x34 =  z Z  
keycode 0x35 =  x X  
keycode 0x36 =  c C  
keycode 0x37 =  v V  
keycode 0x38 =  b B  
keycode 0x39 =  n N  
keycode 0x3a =  m M VoidSymbol mu
keycode 0x3b =  comma apostrophe VoidSymbol masculine
keycode 0x3c =  period period  
keycode 0x3d =  eacute Eacute VoidSymbol usldead_acute
keycode 0x41 =  space space  
keycode 0x5e =  guillemotleft guillemotright VoidSymbol degree
keycode 0x78 =  Mode_switch

clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3

add shift = Shift_L
add shift = Shift_R
add shift = Alt_R
add lock = Caps_Lock
add control = Control_L

add mod1 = Alt_L
add mod1 = Alt_R
add mod1 = Mode_switch
add mod2 = Num_Lock
add mod2 = Mode_switch
add mod3 = Alt_R
add mod3 = Mode_switch

!
! First clear all the modifiers, then set them up as in the default server map
!
clear shift
clear lock
clear control
clear mod1
clear mod2
clear mod3
clear mod4
clear mod5

add shift = Shift_L
add shift = Shift_R
add lock = Caps_Lock
add control = Control_L
add mod1 = Alt_L
add mod2 = Mode_switch

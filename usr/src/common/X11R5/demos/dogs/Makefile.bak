SRCS1=	dogs.c Dog.c Square.c

OBJS1=	dogs.o Dog.o Square.o

UILS1= dogs.uil

PROGRAMS = dogs

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $(_NOOP_) $*.c

 OBJS = $(OBJS1) $(OBJS2) $(OBJS3)
 SRCS = $(SRCS1) $(SRCS2) $(SRCS3)

all:: $(PROGRAMS)

dogs: $(OBJS1) $(DEPLIBS1)
	$(RM) $@
	$(CC) -o $@ $(LDOPTIONS) $(OBJS1) $(LOCAL_LIBRARIES) $(LDLIBS)  $(EXTRA_LOAD_FLAGS)

install:: dogs
	@if [ -d $(DESTDIR)$(BINDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  dogs $(DESTDIR)$(BINDIR)

install.man::
	@echo To install documentation for $(PROGRAMS), consult the Release Notes
	@echo and/or README file in the current directory.

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC); $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

clean::
	$(RM) $(PROGRAMS)

all:: dogs.uid

dogs.uid: $(UILS1) $(DEPUIL)
	$(RM) $@
	$(UIL) $(UILFLAGS) $(LOCAL_UILFLAGS_1) -o $@ dogs.uil $(UIL_INCLUDES)

install:: dogs.uid
	@if [ -d $(DESTDIR)$(UIDDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(UIDDIR)); fi
	$(INSTALL) -c $(INSTDATFLAGS) dogs.uid $(DESTDIR)$(UIDDIR)

clean::
	$(RM) dogs.uid


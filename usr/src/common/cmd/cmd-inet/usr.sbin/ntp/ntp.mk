DIRS = lib in.xntpd xntpdc ntpq ntpdate xntpres

all install clean clobber:
		@for i in $(DIRS);\
		do\
			cd $$i;\
			/bin/echo "\n===== $(MAKE) -f $$i.mk $@";\
			$(MAKE) -f $$i.mk $@ $(MAKEARGS);\
			cd ..;\
		done;\
		wait

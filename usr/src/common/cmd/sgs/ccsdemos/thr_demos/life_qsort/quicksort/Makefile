#ident	"@(#)ccsdemos:thr_demos/life_qsort/quicksort/Makefile	1.1"
all: quicksort qsort

quicksort:	quicksort.c
	cc -lthread -ldl quicksort.c -o quicksort
qsort:		qsort.c
	cc -lthread -ldl qsort.c -o qsort


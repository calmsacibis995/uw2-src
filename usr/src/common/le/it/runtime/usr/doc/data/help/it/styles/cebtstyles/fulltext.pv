<style name="SECTION,TITLE">
	<font-family>	helvetica	</>
	<font-weight>	Bold	</>
	<font-size>	16	</>
	<foreground>	blue	</>
	<left-indent>	52	</>
	<line-spacing>	20	</>
	<space-before>	40	</>
	<justification>	Left	</>
	<break-before>	line	</>
</style>

<style name="SIDEBAR">
	<right-indent>	12	</>
	<hide>	Children	</>
	<script>	ebt-reveal stylesheet="sidebar"	</>
</style>

<style name="SP.ICONSHOW">
	<left-indent>	36	</>
	<space-before>	12	</>
	<icon-position>	Inline	</>
	<break-before>	line	</>
	<icon-type>	attr(TITLE)	</>
</style>

<style name="SP.SUB">
	<vertical-offset>	&subscript	</>
</style>

<style name="SP.TEX">
	<icon-position>	Right	</>
	<hide>	Children	</>
	<break-after>	Line	</>
	<script>	ebt-reveal root=me() stylesheet=texeqn.pri	</>
	<icon-type>	default	</>
</style>

<style name="SPECIAL">
	<font-family>	&title.font	</>
	<select>	SP.toupper(attr(type))	</>
</style>

<style name="SS2,ST">
	<font-family>	helvetica	</>
	<font-weight>	Bold	</>
	<font-size>	10	</>
	<foreground>	blue	</>
	<left-indent>	52	</>
	<line-spacing>	14	</>
	<space-before>	12	</>
	<justification>	Left	</>
	<break-before>	line	</>
</style>

<style name="STYLE">
	<font-family>	helvetica	</>
	<space-before>	6	</>
	<space-after>	6	</>
	<break-before>	Line	</>
	<text-before>if(attr(NAME), Property setting\(s\) for style attr(NAME):, Property setting:)</>
</style>

<style name="STYLE,#TEXT-BEFORE">
	<font-family>	helvetica	</>
	<font-weight>	Bold	</>
	<font-size>	-=1	</>
</style>

<style name="SUBBLOCK,LABEL">
	<font-slant>	Italics	</>
	<space-before>	8	</>
	<justification>	Left	</>
	<break-before>	Line	</>
</style>

<style name="SUBBLOCK,P">
	<space-after>	8	</>
	<break-before>	if(eq(cnum(),1),None,Line)	</>
</style>

<style name="SUBBLOCK,VERBATIM">
	<font-family>	courier	</>
	<font-size>	8	</>
	<left-indent>	+=12	</>
	<space-before>	0	</>
	<space-after>	12	</>
	<justification>	verbatim	</>
	<break-before>	Line	</>
</style>

<style name="SUBTITLE">
	<font-family>	helvetica	</>
	<font-size>	14	</>
	<line-spacing>	20	</>
	<space-before>	10	</>
	<justification>	Left	</>
	<break-before>	line	</>
</style>

<style name="SYMBOL">
	<font-family>	symbol	</>
	<font-weight>	Medium	</>
</style>

<style name="TABLE">
	<space-before>	12	</>
	<break-before>	Line	</>
</style>

<style name="TABLECELL">
	<left-indent>	+=div(tableinfo(arbor,left-indent,5),2)	</>
	<width>	sub(div(tableinfo(arbor,width,5),2),15)	</>
	<justification>	tableinfo(arbor,justification)	</>
	<column>	True	</>
</style>

<style name="TABLECELL,LIST">
	<space-before>	4	</>
	<break-before>	line	</>
</style>

<style name="TABLECELL,P">
	<left-indent>	if(eq(cnum(ancestor()),1), +=5, +=0)	</>
	<right-indent>	if(eq(cnum(ancestor()),1), +=5, +=0)	</>
	<break-before>	if(eq(cnum(ancestor()),1), None, Line)	</>
</style>

<style name="TABLEROW">
	<font-weight>	if(eq(cnum(),1),Bold,Medium)	</>
	<font-slant>	if(eq(cnum(),1),Italics,Roman)	</>
	<space-before>	4	</>
	<space-after>	4	</>
